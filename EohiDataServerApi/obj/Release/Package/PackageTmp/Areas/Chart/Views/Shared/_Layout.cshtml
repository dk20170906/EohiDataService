<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
 
    <meta name="description" content="invoice page" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <!--Basic Styles-->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/css/weather-icons.min.css")" rel="stylesheet" />

    <!--Beyond styles-->
    <link id="beyond-link" href="@Url.Content("~/assets/css/beyond.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/css/demo.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/css/typicons.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/css/animate.min.css")" rel="stylesheet" />
    
    <script src="@Url.Content("~/assets/js/jquery-2.0.3.min.js")"></script>
   
</head>


<!-- Body -->
<body>
    <!-- Loading Container -->
    @RenderBody()
  

    <!--Page Related Scripts-->
    <!-- Base -->
    @*<script src="@Url.Content("~/assets/js/jquery-2.0.3.min.js")"></script>*@
    <script src="@Url.Content("~/assets/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/assets/js/beyond.min.js")"></script>
    <!--Tips Support-->
    @*<script src="@Url.Content("~/assets/js/bootbox/bootbox.js")"></script>*@
    @*<script src="@Url.Content("~/assets/js/toastr/toastr.js")"></script>*@
    <script src="@Url.Content("~/assets/js/echarts.min.js")"></script>


    @RenderSection("scripts", required: false)

</body>
<!--  /Body -->
</html>
