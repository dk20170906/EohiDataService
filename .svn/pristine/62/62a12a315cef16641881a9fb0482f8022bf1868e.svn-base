.editor {
    background: #f7f8fa;
    width: 360px;
    min-height: 100%;
    position: absolute;
    top: 0;
    right: -400px;
    overflow: hidden;
    transition: right .3s;
    z-index: 1024
}

.editor.switch-on {
    right: 0
}

.switch-item {
    position: absolute;
    display: block;
    width: 25px;
    height: 30px;
    z-index: 99999;
    text-decoration: none;
    top: 5px;
    padding: 4px;
    font-size: 12px;
    color: rgba(0,3,51,.5);
    cursor: pointer;
    text-indent: -9999px;
    background: #eaf2f4;
    background-size: 14px;
    background-position: center center;
    background-repeat: no-repeat;
    border: 1px solid rgba(0,0,0,.1)
}

.component-layer-list {
    position: absolute;
    height: calc(100vh);
