    }

    .icon-text-button .icon, .icon-text-button .icon i {
        font-size: 20px !important
    }

    .icon-text-button .text {
        font-size: 12px !important
    }

    .icon-text-button .icon {
        padding-top: 6px;
        padding-bottom: 4px
    }

    .icon-text-button .text {
