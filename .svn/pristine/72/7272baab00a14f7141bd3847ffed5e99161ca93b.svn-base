.table-scoll {
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    transform: translate3d(0px, 0px, 0px);
    opacity: 1;
    font-family: "Microsoft Yahei", Arial, sans-serif;
}

.table-line {
    position: relative;
    perspective: 250px;
    perspective-origin: 50% 100%;
}

.table-content {
    width: 100%;
    height: 30px;
    /*line-height: 30px;*/
    position: relative;
    display: flex;
    vertical-align: bottom;
    /*color: white;
    font-size: 16px;*/
    font-weight: normal;
    overflow: hidden;
    white-space: nowrap;
    flex: auto;
}

    .table-content .scrollbox {
        width: 100%;
        white-space: nowrap;
        overflow: hidden;
        transition: all .2s;
    }

    .table-content .txt, .table-content .txt-clone {
        display: inline;
        position: relative;
        /*font-size:32px;
        color:white;*/
    }

.demo-cont {
    display: flex;
    align-items: center;
    margin-bottom: 8px;
    position: relative;
    transform-style: preserve-3d;
    transform-origin: 50% 50% 0px;
    transform: rotateX(0deg);
    width: 100%;
    height: 24px;
}

.table-index {
    /*color: white;
    font-size: 16px;*/
    font-weight: normal;
    padding-right: 10px;
    width: auto;
    flex: none;
}



.inneranimation {
    transform: rotateX(360deg);
    -webkit-transform: rotateX(360deg);
    transition: transform 1s ease-in-out;
    -webkit-transition: transform 1s ease-in-out;
    -moz-transition: transform 1s ease-in-out;
    -o-transition: transform 1s ease-in-out;
}

    
