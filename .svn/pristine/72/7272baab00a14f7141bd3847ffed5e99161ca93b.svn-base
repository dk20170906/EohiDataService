    }

.left-wrap ul.chart-menu {
    float: right;
    margin: 0 0 0 90px;
    height: 60px
}

    .left-wrap ul.chart-menu .chart-group {
        position: relative;
        width: 60px !important;
        padding: 5px 0;
        cursor: pointer;
        line-height: 0;
        text-align: center;
        height: 60px
    }

        .left-wrap ul.chart-menu .chart-group .chart-group-flex {
            display: flex;
            flex-direction: column;
            text-align: center;
            align-items: center;
            position: relative
        }

            .left-wrap ul.chart-menu .chart-group .chart-group-flex:hover .icon-text-tip {
                display: block
            }

        .left-wrap ul.chart-menu .chart-group .icon-text-tip {
            display: none;
            z-index: 99;
            position: absolute;
            top: 40px
        }

            .left-wrap ul.chart-menu .chart-group .icon-text-tip .icon-text-tip-hide {
                margin-left: 40%;
                width: 0;
                height: 0;
                border-width: 5px 6px;
                border-style: solid;
                border-color: transparent transparent rgba(0,0,0,.7) transparent;
                display: inherit;
            }

            .left-wrap ul.chart-menu .chart-group .icon-text-tip .icon-text-hide {
                font-family: "Microsoft YaHei","Helvetica Neue",Arial,sans-serif !important;
                background: rgba(0,0,0,.7);
                padding: 5px 10px;
                font-size: 10px;
                color: #fff;
                border-radius: 4px;
                white-space: nowrap;
            }

        .left-wrap ul.chart-menu .chart-group a {
            font-size: 12px
        }

        .left-wrap ul.chart-menu .chart-group .chart-single-menu {
            position: absolute;
            top: 60px;
            left: 0;
            display: none;
            background: #373d41;
            box-shadow: 0 2px 2px 0 #000;
            text-align: left;
            padding: 0 10px 15px
        }

            .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-single-menu-list {
                display: flex;
                flex-wrap: wrap
            }

            .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-item {
                line-height: 16px;
                width: 100px;
                overflow: hidden;
                text-overflow: ellipsis;
                color: #aaa;
                padding: 0;
                margin: 15px 5px 0;
                font-size: 0;
                position: relative
            }

                .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-item img {
                    width: 100px;
                    height: 75px
                }

                .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-item p {
                    margin: 5px 0 0 0;
                    text-align: center;
                    padding: 0;
                    font-size: 10px;
                    color: #9b9b9b
                }

                .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-item:hover p {
                    color: #fff
                }

                .left-wrap ul.chart-menu .chart-group .chart-single-menu .chart-item:hover img {
                    border: 1px solid #00c1de
                }

        .left-wrap ul.chart-menu .chart-group .chart-single-menu {
            width: 355px;
            overflow-y: scroll;
            height: auto;
            max-height: 670px
        }

        .left-wrap ul.chart-menu .chart-group.active {
            background: #373d41
        }

            .left-wrap ul.chart-menu .chart-group.active .chart-single-menu {
                display: block
            }

    .left-wrap ul.chart-menu .chart-more .icon-text-button {
