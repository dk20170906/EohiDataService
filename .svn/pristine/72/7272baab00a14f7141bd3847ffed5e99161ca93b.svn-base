/*横向进度条样式*/
.percentage-bar {
    position: relative;
    width: 100%;
    padding: 0 20px;
}

.bar-box {
    box-sizing: border-box;
    height: 60px;
}

.left, .right {
    height: 100%;
}

.percentage-bar-scroll {
    position: relative;
    height: 60px;
    margin: 60px auto 0;
    box-sizing: border-box;
    border-radius: 1px;
    border: 1px solid rgba(255, 255, 255, 0.6);
    padding: 2px;
    height: 60px
}

.percentCount {
    position: absolute;
    padding: 5px;
    top: -50px;
    float: right;
    border: 1px solid rgba(255, 255, 255, 0.6);
    border-radius: 1px;
    transform: translate3d(-50%, 0px, 0px);
}

.progressbar {
    width: 100%;
    height: 100%;
    position: relative;
    background-color: #EEEEEE;
    box-shadow: inset 0px 1px 1px rgba(0,0,0,.1);
    box-sizing: border-box;
}

.proggress {
    height: 8px;
    width: 10px;
    background-color: #3498db;
}

.percentCount {
    color: rgb(255, 255, 255);
}

    .percentCount:after {
        content: '';
        position: absolute;
        bottom: -7px;
        left: 50%;
        margin-left: -5px;
        border-bottom: 1px solid rgba(255, 255, 255, 0.6);
        border-right: 1px solid rgba(255, 255, 255, 0.6);
        width: 11px;
        height: 11px;
        text-align: center;
        border-radius: 0px 0px 1px;
