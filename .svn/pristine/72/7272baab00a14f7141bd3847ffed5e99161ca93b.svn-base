#data-center-panel .data-source {
    background: #464646;
    font-size: 12px
}

    #data-center-panel .data-source .prefix-label {
        display: inline-block;
        line-height: 14px;
        padding: 8px 4px 8px 0
    }

    #data-center-panel .data-source label.for-checkbox {
        border: 1px solid transparent;
        width: 14px;
        height: 14px;
        cursor: pointer;
        position: relative;
        display: inline-block;
        border-radius: 3px;
        top: 4px;
        margin-right: 7px;
        background: #2e2e2e;
        box-sizing: content-box
    }

        #data-center-panel .data-source label.for-checkbox:hover {
            border: 1px solid #00c1de
        }

        #data-center-panel .data-source label.for-checkbox:after {
            content: '';
            width: 8px;
            height: 3px;
            position: absolute;
            top: 4px;
            left: 2px;
            border: 2px solid #00c1de;
            border-top: none;
            border-right: none;
            background: rgba(0,0,0,0);
            opacity: 0;
            -webkit-transform: rotate(-45deg);
            transform: rotate(-45deg);
            box-sizing: content-box
        }

        #data-center-panel .data-source label.for-checkbox.checked {
            border: 1px solid #00c1de
        }

            #data-center-panel .data-source label.for-checkbox.checked:after {
                opacity: 1
            }

    #data-center-panel .data-source input[type=checkbox] {
        visibility: hidden
    }

