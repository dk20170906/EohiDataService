/*顶部导航*/
.navigator {
    position: relative;
    background: #212528;
    height: 60px;
    line-height: 60px;
    padding: 0 20px;
    z-index: 999999;
    display: flex
}

    .navigator .icon-back {
        color: #00c1de;
        font-size: 30px
    }

    .navigator .left a, .navigator .right a {
        font-size: 30px;
        vertical-align: middle
    }

        .navigator .left a:hover, .navigator .right a:hover {
            color: #fff
        }

    .navigator .right .icon i {
        color: #00c1de;
        font-size: 20px !important
    }

    .navigator .right .button-wrapper {
        float: right;
        line-height: normal
    }

        .navigator .right .button-wrapper:hover {
            background: #373d41
        }

        .navigator .right .button-wrapper a:focus {
            color: #fff;
            text-decoration: none
        }

    .navigator .share.shared {
        background-color: #579fcb
    }

    .navigator .left-wrap {
        flex: 1;
        float: left
    }

        .navigator .left-wrap .left {
            float: left
        }

            .navigator .left-wrap .left .go-back {
                padding: 10px;
                height: 60px;
                float: left
            }

                .navigator .left-wrap .left .go-back i {
                    float: left;
                    padding-top: 5px
                }

                .navigator .left-wrap .left .go-back:hover {
                    background: #373d41
                }

    .navigator .right {
        position: absolute;
        right: 24px
    }

.icon-text-button {
    width: 70px;
    height: 60px;
    padding-top: 10px;
    display: block;
    color: #fff;
    text-align: center;
    position: relative
}

    .icon-text-button p {
